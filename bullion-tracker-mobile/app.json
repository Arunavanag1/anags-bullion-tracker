{
  "expo": {
    "name": "TrakStack",
    "slug": "trackstack-mobile",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/434c39cc-3a97-4d3d-8553-b2882aa52a77"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFBF8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trackstack.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFBF8"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.trackstack.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "http://10.0.0.146:3000",
      "metalPriceApiKey": "de370593eb772aeca103b27bb7c8c226",
      "eas": {
        "projectId": "434c39cc-3a97-4d3d-8553-b2882aa52a77"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow TrakStack to access your photos to add images of your collection items.",
          "cameraPermission": "Allow TrakStack to use your camera to take photos of your collection items."
        }
      ],
      "expo-secure-store"
    ]
  }
}
